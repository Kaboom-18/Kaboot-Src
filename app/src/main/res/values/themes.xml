<resources xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppTheme" parent="Theme.Material3.Dark.NoActionBar">

        <item name="colorPrimary">#1F1F1F</item> <!--Toolbar-->
        <item name="colorOnPrimary">#FFFFFF</item> <!--Toolbar Title-->
        <item name="colorPrimaryContainer">#2C2C2C</item> <!--Floating Button-->
        <item name="colorOnPrimaryContainer">#FFFFFF</item> <!--Floating Button Color-->
        <item name="colorSurface">#121212</item> <!--CardView and AlertDialog-->
        <item name="colorOnSurface">#FFFFFF</item> <!--AlertView Title-->
        <item name="colorOnSurfaceVariant">#FFFFFF</item> <!-- AlertView Color-->
        <item name="android:colorBackground">#121212</item> <!--Background-->
        <item name="android:statusBarColor">?attr/colorPrimary</item> <!--Status Bar-->
        <item name="android:navigationBarColor">?attr/colorSurface</item> <!--Navigation Bar-->
        <item name="android:textColor">#FFFFFF</item> <!--TextColot-->
    </style>
</resources>